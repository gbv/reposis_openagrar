<?xml version="1.0" encoding="UTF-8"?>

<MyCoReWebPage>
  <section title="Statistiken" xml:lang="de"/>
  <section title="Statistics" xml:lang="en"/>
  <section xml:lang="all">
    <script src='../../js/statistic.js'> </script>
  </section>

  <section xml:lang="de">
    <div class="row">
      <div class="col-12">
        <h1>Statistik - BfR Genre</h1>
        <p>
          Die Publikationen des BfR nach Jahr, Genre und referiert.
        </p>
        <div
          data-mirelementtype="Statisticsbrowser"
          data-facets='["mods.yearIssued","mods.genre","mods.refereed"]'
          data-fq='["objectType:mods","category.top:\"mir_institutes:bfr\""]'
          data-expandAllElements="true"
          data-hideEmptyElements="true"
          data-lang="de" />
      </div>
    </div>
  </section>

  <section xml:lang="en">
    <div class="row">
      <div class="col-12">
        <h1>Statistic - BfR Genre</h1>
        <p>
          The publications of BfR per year, genre and refeered.
        </p>
        <div
          data-mirelementtype="Statisticsbrowser"
          data-facets='["mods.yearIssued","mods.genre","mods.refereed"]'
          data-fq='["objectType:mods","category.top:\"mir_institutes:bfr\""]'
          data-hideEmptyElement="true"
          data-lang="en" />
      </div>
    </div>
  </section>

</MyCoReWebPage>
