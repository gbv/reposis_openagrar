<?xml version="1.0" encoding="UTF-8"?>

<MyCoReWebPage>
  <section title="Statistiken" xml:lang="de"/>
  <section title="Statistics" xml:lang="en"/>
  <section xml:lang="all">
    <script src='../../js/statistic.js'> </script>
  </section>
     
  <section xml:lang="de">
    <div class="row">
      <h1>Statistik - MRI Genre</h1>
      <p> 
        Die Publikationen des MRI nach Genre und referiert f√ºr die Jahre 2012 bis 2018.
      </p>
      <div  class="row" 
        data-mirelementtype="Statisticsbrowser"
        data-facets='["mods.genre","mods.refereed"]'
        data-horizontalFacet="mods.yearIssued"
        data-fq='["objectType:mods","category.top:\"mir_institutes:mri\"","mods.yearIssued:[2012 TO 2018]"]'
        data-expandAllElements="true"
        data-hideEmptyElements="true"
        data-lang="de"
      >
      </div>
    </div>
  </section>
    
  <section xml:lang="en">
    <div class="row">
      <h1>Statistic - BfR Genre</h1>
      <p> 
        The publications of BfR per year, genre and refeered.
      </p>
      <div  class="row" 
        data-mirelementtype="Statisticsbrowser"
        data-facets='["mods.genre","mods.refereed"]'
        data-horizontalFacet="mods.yearIssued"
        data-fq='["objectType:mods","category.top:\"mir_institutes:bfr\"","mods.yearIssued:[2012 TO 2018]"]'
        data-expandAllElements="true"
        data-hideEmptyElement="true"
        data-lang="en"
      >
      </div>
    </div>
  </section>
  
